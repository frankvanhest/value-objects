{
    "name": "frankvanhest/value-objects",
    "description": "Value Objects interfaces and abstracts to create value objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank van Hest",
            "email": "frank@frankvanhest.nl"
        }
    ],
    "require": {
        "php": "~8.1|~8.2",
        "roave/dont": "^1.6"
    },
    "require-dev": {
        "beberlei/assert": "~v3.3",
        "infection/infection": "^0.26",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15.21",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "FrankVanHest\\ValueObjects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FrankVanHest\\ValueObjects\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
